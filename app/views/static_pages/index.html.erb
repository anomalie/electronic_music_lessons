<div class="row" ng-controller="TutorialsController as app">

  <!-- Repeat this div -->
  <div class="container" ng-repeat="tutorial in app.tutorials">

    <div class="col l4 m6 s12">
      <div class="card medium">
        <div class="card-image">
          <img ng-src="{{tutorial.picture}}">
          <p>{{tutorial.picture}}</p>
          <div id="tutorial-name" class="transparentbox card-title">{{tutorial.name}}</div>
        </div>
        <div class="card-content">
          <p>{{tutorial.description}}</p>
        </div>
        <div class="card-action">
          <a ng-href="tutorials/{{tutorial.id}}">Start Tutorial</a>
        </div>
      </div>
    </div>
  </div> 
</div>


  
